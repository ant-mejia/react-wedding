<html>

<head>
  <meta http-equiv=3D "Content-Type" content=3D "text/html; charset=3Dutf-8" />
  <meta name=3D "viewport" content=3D "width=3Ddevice-width, minimum-scale=3D1.=0, maximum-scale=3D1.0, user-scalable=3D0" />
  <meta name=3D "apple-mobile-web-app-capable" content=3D "yes" />
  <style type="text/css">
    @import url("http://fonts.googleapis.com/css?family=Sacramento|Comfortaa");
    body {
      width: 100%;
      background-color: #f0f2f5;
      margin: 0;
      padding: 0;
      -webkit-font-smoothing: antialiased;
      mso-margin-top-alt: 0px;
      mso-margin-bottom-alt: 0px;
      mso-padding-alt: 0px 0px 0px 0px;
    }

    p,
    h1,
    h2,
    h3,
    h4 {
      margin-top: 0;
      margin-bottom: 0;
      padding-top: 0;
      padding-bottom: 0;
    }

    span.preheader {
      display: none;
      font-size: 1px;
    }

    html {
      width: 100%;
    }

    table {
      font-size: 14px;
      border: 0;
    }

    span.button {
      min-width: 90px;
      padding-left: 40px;
      padding-right: 40px;
    }

    .main-header h1 {
      font-size: 46px;
    }

    .left-text {
      text-align: left;
    }

    .db {
      display: block;
    }
    /* ----------- responsivity ----------- */

    @media only screen and (max-width: 640px) {
      /*------ top header ------ */
      body[yahoo] .show {
        display: block !important;
      }
      body[yahoo] .hide {
        display: none !important;
      }
      body[yahoo] .main-header h1 {
        font-size: 45px !important;
        line-height: 45px !important;
      }
      /*----- main image -------*/
      body[yahoo] .section-img img {
        width: 120px !important;
        height: auto !important;
      }
      /* ====== divider ====== */
      body[yahoo] .divider img {
        width: 440px !important;
      }
      /*--------- banner ----------*/
      body[yahoo] .banner img {
        width: 440px !important;
        height: auto !important;
      }
      /*-------- container --------*/
      body[yahoo] .container590 {
        width: 440px !important;
      }
      body[yahoo] .container580 {
        width: 400px !important;
      }
      body[yahoo] .container1 {
        width: 420px !important;
      }
      body[yahoo] .container2 {
        width: 400px !important;
      }
      body[yahoo] .container3 {
        width: 380px !important;
      }
      /*-------- sections ----------*/
      body[yahoo] .section-item {
        width: 120px !important;
      }
      /*body[yahoo] .section-img img {
        width: 440px !important;
        height: auto !important;
      }*/
    }

    @media only screen and (max-width: 479px) {
      /*------ top header ------ */
      body[yahoo] .main-header h1 {
        font-size: 30px !important;
        line-height: 30px !important;
      }
      body[yahoo] .resize-text {
        font-size: 13px !important;
      }
      /*----- main image -------*/
      body[yahoo] .section-img img {
        width: 75px !important;
        height: auto !important;
      }
      /* ====== divider ====== */
      body[yahoo] .divider img {
        width: 280px !important;
      }
      body[yahoo] .align-center {
        text-align: center !important;
      }
      /*-------- container --------*/
      body[yahoo] .container590 {
        width: 280px !important;
      }
      body[yahoo] .container580 {
        width: 250px !important;
      }
      body[yahoo] .section-img img {
        width: 75px !important;
        height: auto !important;
      }
      /*------- CTA -------------*/
      body[yahoo] .cta-button {
        width: 200px !important;
      }
      body[yahoo] .cta-text {
        font-size: 15px !important;
      }
    }
  </style>
</head>

<body yahoo="fix" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
  <table border="0" width="100%" cellpadding="0" cellspacing="0" bgcolor="f0f2f5" class="body_color" style="background-color: #fff;">
    <tbody>
      <tr>
        <td>
          <table border="0" align="center" width="510" cellpadding="0" cellspacing="0" class="container590">
            <tbody>
              <tr>
                <!-- ======= main img ======= -->
                <td align="center" class="section-img">
                  <span style="display: block; border-style: none !important; border: 0 !important;" class="editable_img">
                    {{> logo src=images.[0] width=160 }}
							    </span>
                </td>
              </tr>
              <tr>
                <td height="15" style="font-size: 15px; line-height: 15px;">&nbsp;</td>
              </tr>
              <tr>
                <td align="center" style="color: rgb(195, 203, 212); font-size: 23px; font-family: Sacramento, sans-serif; line-height: 55px;" class="title_color main-header">
                  <!-- ======= section header ======= -->
                  <div class="editable_text" style="line-height: 55px;">
                    <span class="text_container title">
	        						<h1>{{ title }}</h1>
								    </span>
                  </div>
                </td>
              </tr>
              <tr>
                <td height="25" style="font-size: 25px; line-height: 25px;">&nbsp;</td>
              </tr>
              <tr>
                <td>
                  <table align="center" width="180" border="0" cellpadding="0" cellspacing="0" bgcolor="#c38acc" class="divider_color">
                    <tbody>
                      <tr>
                        <td height="1">
                          <span style="height: 100%; width: 100%; background-color: #c38acc;"></span>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
              <tr>
                <td height="40" style="font-size: 40px; line-height: 40px;">&nbsp;</td>
              </tr>
              <tr>
                <td>
                  <table border="0" width="480" align="center" cellpadding="0" cellspacing="0" class="container580">
                    <tbody>
                      <tr>
                        <td align="center" style="color: rgb(195, 203, 212); font-size: 16px; font-family: Comfortaa, sans-serif; line-height: 32px;" class="resize-text text_color">
                          <div class="editable_text body" style="line-height: 32px">
                            {{#each body }}
                              <span class="text_container left-text db" style="padding-bottom: 20px; text-align: left; display: block;">{{ this }}</span>
                            {{/each}}
                          </div>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
              <tr>
                <td height="50" style="font-size: 50px; line-height: 50px;">&nbsp;</td>
              </tr>
              <tr>
                <td align="center">
                  <table border="0" align="center" width="250" cellpadding="0" cellspacing="0" bgcolor="f06e6a" style="border-radius: 50px; background-color: rgb(225, 203, 113);" class="cta-button main_color">
                    <tbody>
                      <tr>
                        <td height="13" style="font-size: 13px; line-height: 13px;">&nbsp;</td>
                      </tr>
                      <tr>
                        <td align="center" style="color: #ffffff; font-size: 16px; font-family: 'Comfortaa', sans-serif;" class="cta-text">
                          <a href="" style="color: #ffffff; text-decoration: none;">
                            <!-- ======= main section button ======= -->
                            <div class="editable_text" style="line-height: 24px;">
                              <span class="text_container button">
			                    			{{ button }}
		                    	     </span>
                            </div>
                          </a>
                        </td>
                      </tr>
                      <tr>
                        <td height="13" style="font-size: 13px; line-height: 13px;">&nbsp;</td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
              <tr>
                <td height="90" style="font-size: 90px; line-height: 90px;">&nbsp;</td>
              </tr>
              <tr>
                <td align="center" style="color: #afb6c6; font-size: 14px; font-family: 'Comfortaa', sans-serif; line-height: 22px;">
                  <!-- ======= section subtitle ====== -->
                  <div class="editable_text" style="line-height: 22px;"> <span class="text_container">

										<a href="" style="color: #afb6c6; text-decoration: none;" class="text2_color">Our Story</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="" style="color: #afb6c6; text-decoration: none;" class="text2_color">Unsubscribe</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="" style="color: #afb6c6; text-decoration: none;" class="text2_color">Contact</a>

	        					</span> </div>
                </td>
              </tr>
              <tr>
                <td height="40" style="font-size: 40px; line-height: 40px;">&nbsp;</td>
              </tr>
            </tbody>
          </table>
        </td>
      </tr>
    </tbody>
  </table>
</body>

</html>
